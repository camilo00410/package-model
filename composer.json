{
    "name": "fidu/models",
    "description": "Core models for Fidu application",
    "keywords": [
        "fidu",
        "laravel",
        "models"
    ],
    "homepage": "https://github.com/camilo00410/package-model.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Camilo Loaiza",
            "email": "tu@email.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fidu\\Models\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidu\\Models\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidu\\Models\\Providers\\ModelsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}